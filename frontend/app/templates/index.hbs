<div class="flex flex-col items-center justify-center mt-2">
  <Jumbo>
    <h2 class="text-5xl font-bold mt-5 justify-center items-center m-4">All Doctors</h2>

    {{#each @model.doctor as |doctor|}}
      <Doctors @doctor={{doctor}} />
    {{/each}}

    <h2 class="text-5xl font-bold mt-5 justify-center items-center m-4 mt-18" id="practices">All Practices</h2>

    {{#each @model.practice as |practice|}}
      <Practices @practice={{practice}} />
    {{/each}}
  </Jumbo>
</div>