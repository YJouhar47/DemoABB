<Jumbo>
  <h2>Welcome to Super Doctors!</h2>
  <p>We hope you find exactly what you're looking for in a place to stay.</p>
  <LinkTo @route="about" class="button">About Us</LinkTo>
</Jumbo>

{{! <div class="rentals">
  <ul class="results"> }}
{{!-- {{#each @model as |doctor|}}
      <li><Doctor @doctor={{@doctor}} /></li>
    {{/each}} --}}

<form {{on "submit" this.createDoctor}}>
  <dl>
    <dt>Doctor title </dt>
    <dd>
      <Input
        @value={{this.newNaam}}
        @placeholder="Add your doctor's name here"
      />
    </dd>
    <dd>
      <Input @value={{this.newStraat}} @placeholder="978-0374533557" />
    </dd>
  </dl>
  <button type="submit" class="button">Create</button>
</form>

{{! </ul>
</div> }}